{
  "name": "@mcp-funnel/core",
  "version": "0.0.6",
  "author": "Christoph Schramm",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-schra/mcp-funnel.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "!dist/**/*.map"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsx build.ts",
    "build:watch": "tsx build.ts --watch",
    "test": "echo 'ERROR: Tests must be run via `yarn test` from repository root!' && exit 1",
    "release": "yarn npm audit && yarn pack -o out/out.tgz && npm publish --access public ./out/out.tgz"
  },
  "dependencies": {
    "@mcp-funnel/models": "workspace:*",
    "fast-redact": "^3.5.0",
    "pino": "^9.12.0"
  },
  "devDependencies": {
    "@types/fast-redact": "^3.0.4"
  }
}
