{
  "servers": {
    "mockserver": {
      "command": "tsx",
      "args": ["../../e2e/mock-server-runner.ts", "test"],
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "true"
      }
    },
    "github": {
      "command": "tsx",
      "args": ["../../e2e/mock-server-runner.ts", "github"]
    }
  },
  "exposeCoreTools": [
    "discover_tools_by_words",
    "get_tool_schema",
    "bridge_tool_request",
    "load_toolset"
  ],
  "enableDynamicDiscovery": true
}
