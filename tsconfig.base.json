{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "isolatedModules": true,
    "lib": ["esnext", "dom"],
    "moduleResolution": "Bundler",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "types": ["node"],
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  }
}